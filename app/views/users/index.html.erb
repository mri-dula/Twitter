<b><div class="users" ></b>
  <% @users.each do |user| %>
    <div id="user" >
      <% if user.username != current_user.username %>
        <%= h user.username %>
        <%= link_to "Add friend", friendships_path(:friend_id => user), :method => :create %>
      <% end %>
    </div>
  <% end %>
</div>